textures/base_floor/floorpgratecap
{
	qer_editorimage		textures/base_floor/floorpgratecap.tga
	bumpmap			textures/base_floor/floorpgratecap_local.tga
	diffusemap		textures/base_floor/floorpgratecap_d.tga
	specularmap		textures/base_floor/floorpgratecap_s.tga
