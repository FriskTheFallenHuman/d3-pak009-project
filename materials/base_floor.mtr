	qer_editorimage		textures/base_floor/insettrim.tga
	//uncolored verts
	{
		blend 		bumpmap
		map			textures/hell/lfwall27alava_local
	}
	{
		blend 		diffusemap
		map			textures/hell/lfwall27alava_d
		vertexColor
	}
	{
		blend		specularmap
		map			textures/hell/lfwall27alava_s
		vertexColor
	}
	{
		blend		gl_one , gl_one
		map			textures/sfx/radiogoop_red
		rotate		time*.00015
		vertexColor
	}
	{
		blend		gl_one , gl_one
		map			textures/sfx/radiogoop_red
		rotate		time*-.00015
		translate	time*.002,1
		vertexColor
	}
	//black colored verts
	{
		blend 		bumpmap
		map			textures/hell/lfwall27alava_local
	}
	{
		blend 		diffusemap
		map			textures/hell/lfwall27alava_d
		inverseVertexColor
	}
	{
		blend		specularmap
		map			textures/hell/lfwall27alava_s
		inverseVertexColor
	}
}

textures/base_floor/sfloor_burn
{
	ricochet
	qer_editorimage	textures/base_floor/sfloor
	//uncolored verts
	{
		blend 		bumpmap
		map			addnormals( textures/base_floor/sfloor_local.tga, heightmap( textures/base_floor/sfloor_h.tga, 6 ) )
	}
	{
		blend 		diffusemap
		map			textures/base_floor/sfloor_d.tga
		vertexColor
	}
	{
		blend		specularmap
		map			textures/base_floor/sfloor_s.tga
		vertexColor
	}
	//black colored verts
	{
		blend		bumpmap
		map			textures/hell/lfwall27alava_local
	}
	{
		blend		diffusemap
		map			textures/hell/lfwall27alava_d
		inverseVertexColor
	}
	{
		blend		specularmap
		map			textures/hell/lfwall27alava_s
		inverseVertexColor
	}
}