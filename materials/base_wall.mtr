textures/base_wall/lfwall10_zap
{

	noSelfShadow
	translucent
	noShadows

	{
		maskcolor
		map makealpha(textures/base_wall/lfwall10_zap.tga)
		//	scroll	time * 3 , time * 0	
		//clamp
	
	
	}
	{
		blend gl_dst_alpha, gl_one
	        maskalpha
		map textures/base_wall/lfwall10_zap2.tga 
		translate	time * 1 , time * 5

		rgb		mattyzaptable[ time * .1 ] +parm4

	}
		{
		blend gl_dst_alpha, gl_one
	        maskalpha
		map textures/base_wall/lfwall10_zap2.tga 
		scale -1 , -1
		translate	time * 1 , time * 3.5

		rgb		mattyzaptable[ time * .1 ] +parm4

	}

	{
		blend gl_dst_alpha, gl_one
	        maskalpha
		map textures/base_wall/lfwall10_zap3.tga 
		//scale -1 , -1
		translate	time * 1 , time * 0

		scale   1, roachLegs[ time *6 ]
		rgb		mattyzaptable[ time * .1 ] +parm4

	}
}

