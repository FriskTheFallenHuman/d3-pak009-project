textures/mcity/mcity3_grn
{
	qer_editorimage			textures/editor/mcity3_grn.tga
	bumpmap				addnormals (textures/mcity/mcity3_local.tga, heightmap (textures/mcity/mcity3_bmp.tga, 4 ) )
	diffusemap			textures/mcity/mcity3_grn_d.tga
	specularmap			textures/mcity/mcity3_grn_s.tga
	{
		blend			add
		map			textures/mcity/mcity3_add.tga
		rgb			3
	}
}

