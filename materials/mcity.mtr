textures/mcity/mchangar12_skysandnew
{
	qer_editorimage	textures/mcity/mchangar12_skysandnew
	{
		blend 		bumpmap
		map			textures/mcity/mchangar12_local
		scale		2,2
	}
	{
		blend 		diffusemap
		map			textures/mcity/mchangar12
		scale		2,2
		vertexColor
	}
	{
		blend		specularmap
		map			textures/mcity/mchangar12_s
		scale		2,2
		vertexColor
	}
	{
		blend		bumpmap
		map			addnormals (textures/rock/skysand1_local.tga, heightmap (textures/rock/skysand2_bmp.tga, 4))
		//scale		.5, .5
	}
	{
		blend		diffusemap
		map			textures/rock/skysand2.tga
		//scale		.5, .5
		inverseVertexColor
	}
}

