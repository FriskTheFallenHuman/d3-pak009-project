textures/decals/bfg_wallmark
{
	qer_editorimage textures/editor/dontuse.tga
	//sort			farthest
	DECAL_MACRO
	decalInfo 0.5 10 ( 1 1 1 10 ) ( -5 -5 -5 0 )
	{
		blend		gl_zero, gl_one_minus_src_alpha
		map			makealpha(models/weapons/bfg/bfg_wallmark_dirt.tga)
		clamp
		vertexColor
    }
	{
		blend		add
		map			models/weapons/bfg/bfg_wallmark
		rgb			.5 * (fadeintable[ time * .5]) + .5
		vertexColor
	}
}

