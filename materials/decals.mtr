leftSlash
{
	noshadows
	translucent

/*	{
                program         heatHazeWithMaskAndVertex.vfp
                vertexParm              0               0                               // texture scrolling
                vertexParm              1               .2                             // magnitude of the distortion
                fragmentMap             0               _currentRender
                fragmentMap     		1       textures/decals/slash01_vp  // the normal map for distortion
                fragmentMap             2       textures/decals/slash01_vpalpha.tga   // the distortion blend map
      }
*/
	{
		blend  blend
		map    textures/decals/slash01.tga
		colored
	}

}

rightSlash
{
	noshadows
	translucent
/*	{
                program         heatHazeWithMaskAndVertex.vfp
                vertexParm              0               0                               // texture scrolling
                vertexParm              1               .2                             // magnitude of the distortion
                fragmentMap             0               _currentRender
                fragmentMap     		1       textures/decals/slash01b_vp  // the normal map for distortion
                fragmentMap             2       textures/decals/slash01b_vpalpha.tga   // the distortion blend map
    }
*/
	{
		blend  blend
		map    textures/decals/slash01.tga
		scale	-1, 1
		colored
	}

}

