//================textures/decals shader stats===========================================
//
// Total # of shaders : 372
// Working shaders: 212
// Broken/CommentedOut shaders: 139
// Unwisely named shaders which can't be used for map texturing and kept by necessity: 21
// (IOW, all materials with a vertexcolor parameter can only be used by game models)
//=======================================================================================


// this will be full intensity for 90% of the time, then fade to 0
// parm3 is set to the time the mark is spawned
// parm4 is set to the time the mark will be removed
table decalFade { clamp { 1, 1, 1, 1, 1, 1, 1, 1, 1, 0 } }
table brkTable { { 0.3, 1, 1, 1 } }
table roachLegs { snap { -1, 1 } }
table wormTable { { 1,1.2 } }
table woundFade { clamp { 1, 0 } }
table bugwings  { snap { -1, 1 } }
table telemars  {{ 1, .5, .1, .05, .2, .4, .7, .8 }}



textures/decals/bloodyfilmred
{
	noshadows
	translucent

	qer_editorimage		textures/editor/bloodyfilm1.tga
	
		{
		program	colorProcess.vfp
		vertexParm		0	  1   // sinTable[ time * 0.5 ]	// 0.0 = source color, 1.0 = target color
		vertexParm		1		1.2, .0, .0	// target full intensity RGB
		fragmentMap		0		_currentRender
	}
	{

	    blend		add
		scale 2,2
		translate time * staticatable[ time * 10 ], time *staticatable[ time * 10 ]
		rotate time * 7
		map    textures/decals/greystatica.tga

	}

	{

	    blend		filter
		scale 2,2
		translate time * staticatable[ time * 20 ],time * staticatable[ time * 20 ]
		rotate time * 6
		map    textures/decals/oldfilm.tga

	}
	{

	    blend		filter
		scale 1, .007
		translate time * 0, time * -40

		map    textures/decals/oldfilm3.tga

	}
}

textures/decals/telemars


{
	

	noShadows
	twoSided
	qer_editorimage textures/decals/telemars.tga

	{
		blend	diffuseMap
  		map     textures/decals/telemars.tga
		alphatest 0.5
	}
	{
		blend	add
  		map     textures/decals/telemars_add.tga
		rgb		telemars[ time * 1 ]

	}     
      	{
		blend	add
  		map     textures/decals/telemars_b_add.tga
		rgb		telemars[ time * .3 ]
		
		rotate time * .025

	} 
   	{
		blend	add
  		map     textures/decals/telemars_c_add.tga
		rgb		telemars[ time * .1 ]
		
		rotate time * -.035

	} 			
	 	bumpmap	textures/decals/telemars_local.tga
        	specularmap	textures/decals/telemars_s.tga
        


}



