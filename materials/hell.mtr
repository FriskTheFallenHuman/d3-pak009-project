textures/hell/lavatube
{
	qer_editorimage		textures/hell/lava1movenoshadow.tga
	 noSelfShadow
	    noshadows
	    	
	{
		blend 		diffusemap
		map		textures/hell/lava1.tga
		translate	time * 0 , time * -0.1
		red	1
		green	.5
		blue	.65

	} 
	{
		blend 		bumpmap
		map		addnormals( textures/hell/lava1_local.tga, heightmap( textures/hell/lava1_h.tga, 9 ) )
		translate	time * 0 , time * -0.1
	} 
	{
		blend 		specularmap
		map		textures/hell/lava1_s.tga
	}   
	{
		maskcolor
		map textures/hell/lava1.tga
		translate	time * 0 , time * -0.1
		red	1
		green	.5
		blue	.65
	}
	{
		blend gl_dst_alpha , gl_one_minus_dst_alpha
		map textures/hell/lava1fx.tga
		rgb		fireballtable[ time * .6 ]
		translate	time * 0.02 , time * 0.1
	}
	{
		blend add
		map textures/hell/lava1fx.tga
		rgb		fireballtable[ time * .5 ]
		translate	time * 0 , time * -0.1
		red	1
		green	.5
		blue	.65
	}
 }

